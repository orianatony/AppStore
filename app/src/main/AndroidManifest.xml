<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.store.appstore">

    <application
        android:name="com.store.appstore.CarroCompra"
        android:allowBackup="true"
        android:icon="@drawable/cocker"
        android:label="@string/app_name"
        android:roundIcon="@drawable/tienda_cocker_icono"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".HomeAppStore"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeCafe"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".HomeDesayuno"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ProductDetail"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".HomeSale"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>